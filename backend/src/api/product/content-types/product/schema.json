{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "desc": {
      "type": "text",
      "required": true
    },
    "img": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "catagories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::catagory.catagory",
      "inversedBy": "products"
    }
  }
}
